<h1>All Article</h1>
<ul id="articles">
  <% @articles.each do |article| %>
  <li>
    <span>
    <%= link_to article.title, article_path(article), class: 'artile_title', id: "article_#{article.id}" %>
    </span>
  </li>
  <% end %>
</ul>

<%= link_to "Create new article", new_article_path, class: 'new_article' %>


